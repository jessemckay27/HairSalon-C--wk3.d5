@{
	Layout="Shared/_Layout.cshtml";
}
<h1>Current Stylists</h2>
<ul>
  @foreach(var stylist in Model)
  {
    <li>
      <a href="/stylist/@stylist.GetId()">@stylist.GetName()</a> |
      <a href="/stylist/edit/@stylist.GetId()">Edit</a> |
      <a href="/stylist/delete/@stylist.GetId()">Delete</a>
    </li>
    <br>
  }
</ul>
<p><a href="/stylists/new">Add New Stylists</a></p>
<p><a href="/">Home</a></p>
